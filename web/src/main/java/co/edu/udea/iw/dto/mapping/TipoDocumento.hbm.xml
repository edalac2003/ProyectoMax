<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="co.edu.udea.iw.dto.TipoDocumento" table="tipo_documento">
        <id name="idtipoDocumento" type="java.lang.Integer">
            <column name="idtipo_documento" />
            <generator class="identity" />
        </id>
        <property name="descripcion" type="string">
            <column name="descripcion" length="64" />
        </property>
        <property name="sigla" type="string">
            <column name="sigla" length="8" />
        </property>
    </class>
</hibernate-mapping>
